{
    "name": "steamdevportfolio-nextjs-frontend-app",
    "description": "About AI-powered chatbot built with Next.js, Prisma, PostgreSQL, and NextAuth.js (Google OAuth). Supports persistent chat history, memory handling via LangChain/manual prompts, and integration with multiple LLM APIs (Gemini, Gemma, OpenAI, Groq). Simple, flexible, and extensible. Topics",
    "author": "Halim Putra",
    "license": "MIT",
    "keywords": [
        "nextjs",
        "tailwindcss",
        "shadcn-ui",
        "postgresql",
        "groq-api"
    ],
    "repository": {
        "type": "git",
        "url": "https://github.com/bforbilly24/steamdevportfolio-nextjs-frontend-app"
    },
    "version": "3.1.0",
    "private": true,
    "scripts": {
        "dev": "next dev --turbo",
        "build": "bunx prisma generate && bunx prisma migrate deploy && next build",
        "start": "next start",
        "lint": "next lint && biome lint --write --unsafe",
        "lint:fix": "next lint --fix && biome lint --write --unsafe",
        "format": "biome format --write",
        "db:generate": "bunx prisma generate",
        "db:migrate": "bunx prisma migrate dev",
        "db:studio": "bunx prisma studio",
        "db:push": "bunx prisma db push",
        "db:pull": "bunx prisma db pull",
        "db:reset": "bunx prisma migrate reset",
        "db:deploy": "bunx prisma migrate deploy",
        "test": "export PLAYWRIGHT=True && bun exec playwright test"
    },
